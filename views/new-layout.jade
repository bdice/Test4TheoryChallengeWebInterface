            .row.c-service
              #service-volunteers.col-xs-6.col-sm-4.col-md-4
                div
                  .c-service-icon
                    .c-icons.c-icon-1
                  .c-service-desc
                    .marginbot-20
                      h5 Volunteers
                      .divider-header
                    .row
                      .col-sm-8.col-sm-offset-2
