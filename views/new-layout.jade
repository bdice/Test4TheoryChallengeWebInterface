                      .divider-header
                    .row
                      .col-sm-8.col-sm-offset-2
                        h6#status-jobs.h-bold.c-status.c-warning STALE
                    table.table.table-condensed
                      tbody
                        tr
                          th Pending:
                          td.lbl-pending 14,348,907
                        tr
                          th Completed:
                          td.lbl-completed 1,931,221
                        tr
